// This is the output generated by the T4 template
namespace DMS.GLSL
{
	using System.Collections.Generic;

	public static partial class GlslSpecification
	{
		private static Dictionary<string, DefinedWordType> Initialize()
		{
			var keywords = "<#=read("glslKeywords.txt")#>".Split();
			var functions = "<#=read("glslFunctions.txt")#>".Split();
			var variables = "<#=read("glslVariables.txt")#>".Split();
			var result = new Dictionary<string, DefinedWordType>();
			result.AddRange(keywords, DefinedWordType.Keyword);
			result.AddRange(functions, DefinedWordType.Function);
			result.AddRange(variables, DefinedWordType.Variable);
			return result;
		}
	}
}
